<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="dailysalesreport" extends="struts-default">
			
			<action name="printdailysalesreport" class="com.dashboard.project.dailysalesreport.DailySalesReportAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>  
			</action>
			
			<action name="printsummaryreport" class="com.dashboard.project.dailysalesreport.DailySalesReportAction" method="printSumAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>  
			</action>   
			
		</package>	   
</struts>   