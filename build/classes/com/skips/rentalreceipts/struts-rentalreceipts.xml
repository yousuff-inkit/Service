<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="rentalReceipts" extends="struts-default">
			<action name="saveRentalReceipt" class="com.skips.rentalreceipts.ClsRentalReceiptsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >rentalReceipts.jsp</result>
				<result name="fail">rentalReceipts.jsp</result>
			</action>
			
			<action name="printRentalReceipt" class="com.skips.rentalreceipts.ClsRentalReceiptsAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">receiptVoucher.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			<action name="rrjspToPdf" class="com.skips.rentalreceipts.ClsRentalReceiptsAction" method="emailAction">
				<interceptor-ref name="params"/>
			</action>
	
		
		
		</package>	
</struts>