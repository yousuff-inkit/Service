<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="deliveryreturn" extends="struts-default">
			<action name="saveActiondeliveryNoteReturn" class="com.sales.Sales.deliverynotereturn.ClsDeliverynoteReturnAction" method="savedeliveryReturnAction">
				<interceptor-ref name="params"/>
				<result name="success" >deliveryNoteReturn.jsp</result>
				<result name="fail">deliveryNoteReturn.jsp</result>
			</action>
			<action name="printdeliveryNotereturn" class="com.sales.Sales.deliverynotereturn.ClsDeliverynoteReturnAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >deliverynotereturnprint.jsp</result>
			 
			</action>
			
		</package>	
		
		
		
</struts>

