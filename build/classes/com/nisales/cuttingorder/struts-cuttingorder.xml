<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="cor" extends="struts-default">
			<action name="savecor" class="com.nisales.cuttingorder.ClsCuttingOrderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >cuttingorder.jsp</result>
				<result name="fail">cuttingorder.jsp</result>
			</action>
			<action name="printcor" class="com.nisales.cuttingorder.ClsCuttingOrderAction" method="printAction">
				<result name="print" >${url}</result>
			</action>
			
			
			
		</package>	
</struts>