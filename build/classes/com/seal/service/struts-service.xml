<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="service" extends="struts-default" namespace="/">
			<action name="saveservicemasterSeal" class="com.seal.service.ClsServiceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >service.jsp</result>
				<result name="fail">service.jsp</result>
			</action>
		</package>	  
</struts>
