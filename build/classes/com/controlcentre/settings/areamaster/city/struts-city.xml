<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveCity" />

	<package name="city" extends="struts-default">
			<action name="saveCity" class="com.controlcentre.settings.areamaster.city.ClsCityAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">city.jsp</result>
				<result name="fail">city.jsp</result>
			</action>
			
			 <action name="searchDetails" class="com.controlcentre.settings.areamaster.city.ClsCityAction" method="searchDetails">
				<interceptor-ref name="params"/>
				<result name="success" >city.jsp</result>
				<result name="fail">city.jsp</result>
			</action>  
		</package>	
</struts>
