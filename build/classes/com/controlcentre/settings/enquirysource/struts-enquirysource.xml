<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="enquirysource" />

	<package name="enquirysource" extends="struts-default">
			<action name="saveEnquirysource" class="com.controlcentre.settings.enquirysource.ClsEnquirySourceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">enquirySource.jsp</result>
				<result name="fail">enquirySource.jsp</result>
			</action>
			 
		</package>	
</struts>