<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveGroup" />
 
	<package name="groups" extends="struts-default">
			<action name="saveGroup" class="com.controlcentre.masters.dumpmaster.groups.ClsGroupsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >groups.jsp</result>  
				<result name="fail">groups.jsp</result>
			</action>
			
			
		</package>	
</struts>

