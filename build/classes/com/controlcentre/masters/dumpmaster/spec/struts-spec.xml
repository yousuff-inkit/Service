<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveSpec" />
 
	<package name="spec" extends="struts-default">
			<action name="saveSpec" class="com.controlcentre.masters.dumpmaster.spec.ClsSpecAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >spec.jsp</result>
				<result name="fail">spec.jsp</result>
			</action>
			
			
		</package>	
</struts>

