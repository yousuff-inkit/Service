<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Purchase" extends="struts-default">
			<action name="saveActionNipurchase" class="com.finance.nipurchase.nipurchase.ClsnipurchaseAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >niPurchase.jsp</result>
				<result name="fail">niPurchase.jsp</result>
			</action>
			
			<action name="printniphs1" class="com.finance.nipurchase.nipurchase.ClsnipurchaseAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>

			</action>
			<action name="PRINTniphs" class="com.finance.nipurchase.nipurchase.ClsnipurchaseAction" method="jasperprintAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print" >niprint.jsp</result> -->

			</action>
			
			
			
			
		</package>	
</struts>