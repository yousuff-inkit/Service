<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="ibBankPayment" extends="struts-default">
			<action name="saveIbBankPayment" class="com.finance.interbranchtransactions.ibbankpayment.ClsIbBankPaymentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >ibBankPayment.jsp</result>
				<result name="fail">ibBankPayment.jsp</result>
			</action>
			
			<action name="printIBBankPayment" class="com.finance.interbranchtransactions.ibbankpayment.ClsIbBankPaymentAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/ibBankVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
			<action name="printIBBankPaymentCheque" class="com.finance.interbranchtransactions.ibbankpayment.ClsIbBankPaymentAction" method="printChequeAction">
				<interceptor-ref name="params"/>
				<result name="print">../../../../com/finance/chequePrintingFormat.jsp</result>
			</action>
		</package>	
</struts>