<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="enquiryfurniture" extends="struts-default">
			<action name="saveEnquiryfurniture" class="com.operations.marketing.enquiryfurniture.ClsEnquiryFurnitureAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >enquiryfurniture.jsp</result>
				<result name="fail">enquiryfurniture.jsp</result>
			</action>
			<action name="saveCQfurniture" class="com.operations.marketing.enquiryfurniture.ClsEnquiryFurnitureAction" method="saveCQAction">
				<interceptor-ref name="params"/>
				<result name="success" >c_questype.jsp</result>
				<result name="fail">c_questype.jsp</result>
			</action>
			<action name="saveKQfurniture" class="com.operations.marketing.enquiryfurniture.ClsEnquiryFurnitureAction" method="saveKQAction">
				<interceptor-ref name="params"/>
				<result name="success" >k_questype.jsp</result>
				<result name="fail">k_questype.jsp</result>
			</action>
		</package>	
</struts>