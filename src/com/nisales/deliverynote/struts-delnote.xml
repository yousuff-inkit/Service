<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="dly" extends="struts-default">
			<action name="savedly" class="com.nisales.deliverynote.ClsDeliveryNoteAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >deliverynote.jsp</result>
				<result name="fail">deliverynote.jsp</result>
			</action>
			
			<action name="printdly" class="com.nisales.deliverynote.ClsDeliveryNoteAction" method="printAction">
				<result name="print" >${url}</result>
			</action>
			
			
		</package>	
</struts>