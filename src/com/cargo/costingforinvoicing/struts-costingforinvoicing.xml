<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" /> 
	<package name="cif" extends="struts-default">
		<action name="saveCFI" class="com.cargo.costingforinvoicing.ClsCostingForInvoicingAction" method="saveForm"  >
		<interceptor-ref name="params"/>
			<result name="success">costingforinvoicing.jsp</result>
			<result name="fail">costingforinvoicing.jsp</result>
		</action>
        <action name="printCosting" class="com.cargo.costingforinvoicing.ClsCostingForInvoicingAction" method="printAction">
			<interceptor-ref name="params"/>
			<result name="print" >${url}</result>
		</action>
	</package>
</struts>

