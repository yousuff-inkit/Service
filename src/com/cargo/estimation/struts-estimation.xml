<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="cargoEstimation" extends="struts-default">
		<action name="savecargoEstimation" class="com.cargo.estimation.ClsEstimationAction" method="saveAction">
			<interceptor-ref name="params"/>
			<result name="success" >estimation.jsp</result>
			<result name="fail">estimation.jsp</result>
		</action>
		<action name="printEstimation" class="com.cargo.estimation.ClsEstimationAction" method="printAction">
			<interceptor-ref name="params"/>
			<result name="print" >${url}</result>
		</action>
	</package>	
</struts>