<struts>
	<constant name="struts.devMode" value="true" />
	 
	<package name="Appraisal" extends="struts-default">
			<action name="saveAppraisal" class="com.humanresource.transactions.appraisal.ClsAppraisalAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >appriaisal.jsp</result>
				<result name="fail">appriaisal.jsp</result>
			</action>
		
			
		</package>	
</struts>