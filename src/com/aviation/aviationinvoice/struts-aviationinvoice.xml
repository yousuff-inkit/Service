<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="saveavinvsss" extends="struts-default">
			<action name="saveavinv" class="com.aviation.aviationinvoice.ClsaviationInvoiceAction" method="savedata">
				<interceptor-ref name="params"/>
				<result name="success">aviationinvoice.jsp</result>
				<result name="fail">aviationinvoice.jsp</result>
			</action>
			
			<action name="printAviationInvoice" class="com.aviation.aviationinvoice.ClsaviationInvoiceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>
			</action>
		</package>	
</struts>