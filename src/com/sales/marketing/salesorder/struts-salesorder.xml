<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="SalesOrder" extends="struts-default">
			<action name="saveSalesOrder" class="com.sales.marketing.salesorder.ClsSalesOrderAction" method="saveOrderAction">
				<interceptor-ref name="params"/>
				<result name="success" >salesOrder.jsp</result>
				<result name="fail">salesOrder.jsp</result>
			</action>
			
						
 	 <action name="printsalesorder" class="com.sales.marketing.salesorder.ClsSalesOrderAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>  
			
	 </package>	
		
		
</struts>