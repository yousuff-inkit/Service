<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="stockorder" extends="struts-default">
			<action name="saveStockOrder" class="com.sales.marketing.stockorder.ClsStockOrderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >stockorder.jsp</result>
				<result name="fail">stockorder.jsp</result>
			</action>
			
 	<action name="printPurchaseReq" class="com.sales.marketing.stockorder.ClsStockOrderAction" method="printAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>
			
			
		</package>	
		
		
		
</struts>