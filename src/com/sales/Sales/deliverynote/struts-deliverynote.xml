<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="deliveryNote" extends="struts-default">
			<action name="savedeliveryNote" class="com.sales.Sales.deliverynote.ClsDeliveryNoteAction" method="savedeliveryNoteAction">
				<interceptor-ref name="params"/>
				<result name="success" >deliveryNote.jsp</result>
				<result name="fail">deliveryNote.jsp</result>
			</action>
			<action name="printdeliveryNote" class="com.sales.Sales.deliverynote.ClsDeliveryNoteAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>
			 
			</action>
		</package>	
		
		
		
</struts>