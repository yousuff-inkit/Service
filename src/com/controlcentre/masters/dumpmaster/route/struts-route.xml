<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveRoute" />
 
	<package name="route" extends="struts-default">
			<action name="saveRoute" class="com.controlcentre.masters.dumpmaster.route.ClsRouteAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >route.jsp</result>
				<result name="fail">route.jsp</result>
			</action>
			
			
		</package>	
</struts>
