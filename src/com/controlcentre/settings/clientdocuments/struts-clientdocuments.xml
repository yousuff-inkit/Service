<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />

	<package name="clientdocuments" extends="struts-default">
			<action name="saveClientDocuments" class="com.controlcentre.settings.clientdocuments.ClsClientdocumentsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >clientdocuments.jsp</result>
				<result name="fail">clientdocuments.jsp</result>
			</action>
		</package>	
</struts>