<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="saveRegion" />

	<package name="region" extends="struts-default">
			<action name="saveRegion" class="com.controlcentre.settings.areamaster.region.ClsRegionAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">region.jsp</result>
				<result name="fail">region.jsp</result>
			</action>
			
			<action name="searchDetails" class="com.controlcentre.settings.areamaster.region.ClsRegionAction" method="searchDetails">
				<interceptor-ref name="params"/>
				<result name="success" >region.jsp</result>
				<result name="fail">region.jsp</result>
			</action> 
		</package>	
</struts>
