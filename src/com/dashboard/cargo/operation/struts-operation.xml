<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
   
	<package name="opertion" extends="struts-default">    
			
			<action name="printoperation" class="com.dashboard.cargo.operation.ClsOpertionAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>       
			</action>
			<action name="printjobcard" class="com.dashboard.cargo.operation.ClsOpertionAction" method="printJCAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>       
			</action>
			<action name="printtruckcn" class="com.dashboard.cargo.operation.ClsOpertionAction" method="printTCNAction">
				<interceptor-ref name="params"/>
				<result name="print">${url}</result>             
			</action>
			
		</package>	   
</struts>   