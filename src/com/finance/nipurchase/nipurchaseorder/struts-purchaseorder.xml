<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="Purorder" extends="struts-default">
			<action name="saveActionNipurOrder" class="com.finance.nipurchase.nipurchaseorder.ClsnipurchaseorderAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >niPurchaseOrder.jsp</result>
				<result name="fail">niPurchaseOrder.jsp</result>
			</action>
					<action name="printniphOrder" class="com.finance.nipurchase.nipurchaseorder.ClsnipurchaseorderAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>

			</action>
			<action name="PRINTniphOrder" class="com.finance.nipurchase.nipurchaseorder.ClsnipurchaseorderAction" method="jasperprintAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print" >nipurchaseorderprint.jsp</result> -->

			</action>
			
		</package>	
</struts>