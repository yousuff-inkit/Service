<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="creditinvlogin" extends="struts-default">
			<action name="saveCreditInvLogin" class="creditinvlogin.ClsCreditInvLoginAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >dashboard.jsp</result>
				<result name="fail">index.jsp</result>
			</action>
			<action name="printCreditInvLogin" class="creditinvlogin.ClsCreditInvLoginAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >creditinv.jrxml</result>
			</action>
		</package>	
</struts>