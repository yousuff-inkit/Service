<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="enquiry" extends="struts-default">

		<action name="cargoenquiry" class="com.cargo.enquiry.ClscargoEnquiryAction" method="saveCargoEnquiry">
		<interceptor-ref name="params"/>
			<result name="success">enquiry.jsp</result>
			<result name="fail">enquiry.jsp</result>
		</action>

	</package>
</struts>

