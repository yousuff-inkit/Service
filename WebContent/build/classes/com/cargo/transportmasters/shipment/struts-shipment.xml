<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="shipment" extends="struts-default">
			<action name="saveShipment" class="com.cargo.transportmasters.shipment.ClsShipmentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >shipment.jsp</result>
				<result name="fail">shipment.jsp</result>
			</action>
			
		</package>	
</struts>