<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 <constant name="struts.custom.i18n.resources" value="servicesetting" />

	<package name="servicesetting" extends="struts-default">
			<action name="saveServiceSetting" class="com.controlcentre.settings.servicesettings.ClsServiceSettingAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">serviceSettings.jsp</result>
				<result name="fail">serviceSettings.jsp</result>
			</action>
			
			<action name="saveChargingMaster" class="com.controlcentre.settings.servicesettings.ClsServiceSettingAction" method="saveChargingMasterAction">
				<interceptor-ref name="params"/>
				<result name="success">chargingMaster.jsp</result>
				<result name="fail">chargingMaster.jsp</result>
			</action>
			
		</package>	
</struts>
