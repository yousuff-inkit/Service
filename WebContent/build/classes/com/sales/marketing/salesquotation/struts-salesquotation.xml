<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="salesQuotation" extends="struts-default">
			<action name="saveQuotationOld" class="com.sales.marketing.salesquotation.ClsSalesQuotationAction" method="saveQuotationAction">
				<interceptor-ref name="params"/>
				<result name="success" >quotation.jsp</result>
				<result name="fail">quotation.jsp</result>
			</action>
			
		
		 <action name="printSalesQuotAction" class="com.sales.marketing.salesquotation.ClsSalesQuotationAction" method="printQuotAction">
			    <interceptor-ref name="params"/>
			    <result name="print">${url}</result>
			</action>
		
		</package>	
</struts>