<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="BrachInvoice" extends="struts-default">
			<action name="saveBranchInvoice" class="com.sales.Sales.branchInvoice.ClsBranchInvoiceAction" method="saveInvoiceAction">
				<interceptor-ref name="params"/>
				<result name="success">branchinvoice.jsp</result>
				<result name="fail">branchinvoice.jsp</result>
			</action>
			 <action name="printBRinv" class="com.sales.Sales.branchInvoice.ClsBranchInvoiceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >invoiceprint.jsp</result>
				</action>	  
		</package>	
 
</struts>