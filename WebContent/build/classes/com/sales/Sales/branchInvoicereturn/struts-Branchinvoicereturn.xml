<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="BrInvoiceReturn" extends="struts-default">
			<action name="saveBranchInvoiceReturn" class="com.sales.Sales.branchInvoicereturn.ClsbranchInvoicereturnAction" method="saveInvoiceReturnAction">
				<interceptor-ref name="params"/>
				<result name="success" >branchinvoiceReturn.jsp</result>
				<result name="fail">branchinvoiceReturn.jsp</result>
			</action>
			
			<action name="printbranchinvret" class="com.sales.Sales.branchInvoicereturn.ClsbranchInvoicereturnAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >invreturnprint.jsp</result>
				</action>
			
	 </package>
		
		
		
</struts>