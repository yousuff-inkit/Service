<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="deliverynotewithprice" extends="struts-default">
			<action name="savedeliveryNoteWithPrice" class="com.sales.Sales.deliverynotewithprice.ClsDeliveryNoteWithPriceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >delnotewithprice.jsp</result>
				<result name="fail">delnotewithprice.jsp</result>
			</action>
			<action name="printdeliveryNoteWithPrice" class="com.sales.Sales.deliverynotewithprice.ClsDeliveryNoteWithPriceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>
			 
			</action>
		</package>	
		
		
		
</struts>