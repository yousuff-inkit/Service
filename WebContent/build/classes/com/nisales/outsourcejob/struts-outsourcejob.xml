<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="outsourcejob" extends="struts-default">
			<action name="saveoutj" class="com.nisales.outsourcejob.ClsOutSourceJobAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >outsourcejob.jsp</result>
				<result name="fail">outsourcejob.jsp</result>
			</action>
			<action name="printoutj" class="com.nisales.outsourcejob.ClsOutSourceJobAction" method="printAction">
				<result name="print" >${url}</result>
			</action>
			
			
			
		</package>	
</struts>