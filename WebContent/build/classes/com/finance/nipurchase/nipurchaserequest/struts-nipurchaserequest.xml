<struts>
	<constant name="struts.devMode" value="true" />
	
	<package name="Purrequest" extends="struts-default">
		<action name="saveActionNipurRequest" class="com.finance.nipurchase.nipurchaserequest.ClsnipurchaserequestAction" method="saveAction">
			<interceptor-ref name="params"/>
			<result name="success" >niPurchaseRequest.jsp</result>
			<result name="fail">niPurchaseRequest.jsp</result>
		</action>
		<action name="printNiPurchaeRequest" class="com.finance.nipurchase.nipurchaserequest.ClsnipurchaserequestAction" method="printAction">
			<interceptor-ref name="params"/>
			<result name="print" >${url}</result>
		</action>
	</package>	
</struts>