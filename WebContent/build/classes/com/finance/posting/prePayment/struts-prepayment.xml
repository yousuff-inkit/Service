<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="PrePayment" extends="struts-default">
			<action name="savePrePayment" class="com.finance.posting.prePayment.ClsPrePaymentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">prePayment.jsp</result>
				<result name="fail">prePayment.jsp</result>
			</action>
			
		</package>	
</struts>

