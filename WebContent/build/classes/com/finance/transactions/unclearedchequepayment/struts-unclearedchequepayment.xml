<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="unclearedChequePayment" extends="struts-default">
			<action name="saveUnclearedChequePayment" class="com.finance.transactions.unclearedchequepayment.ClsUnclearedChequePaymentAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">unclearedChequePayment.jsp</result>
				<result name="fail">unclearedChequePayment.jsp</result>
			</action>
			
		   <action name="printUnclearedChequePayment" class="com.finance.transactions.unclearedchequepayment.ClsUnclearedChequePaymentAction" method="printAction">
				<interceptor-ref name="params"/>
				<!-- <result name="print">../../../../com/finance/unclearedChequeVoucherPrint.jsp</result> -->
				<result name="print">${url}</result>
			</action>
			
			<action name="printUnclearedChequePaymentCheque" class="com.finance.transactions.unclearedchequepayment.ClsUnclearedChequePaymentAction" method="printChequeAction">
				<interceptor-ref name="params"/>
				<result name="print">../../../../com/finance/chequePrintingFormat.jsp</result>
			</action> 
		</package>	
</struts>