<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="activityreport" extends="struts-default">
			<action name="saveActivityReport" class="com.project.leadmanagement.activityreport.ClsActivityReportAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">activityReport.jsp</result>
				<result name="fail">activityReport.jsp</result>
			</action>
			
		</package>	
</struts>