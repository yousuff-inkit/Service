<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="proformainvoice" extends="struts-default" namespace="/">
			<action name="saveproforma" class="com.project.execution.proformaInvoice.ClsProformaInvoiceAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >proformainvoice.jsp</result>
				<result name="fail">proformainvoice.jsp</result>
			</action>
			
			
			<action name="PRINTServiceSalepro" class="com.project.execution.proformaInvoice.ClsProformaInvoiceAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print" >${url}</result>

			</action>
			
			
				
		</package>	
</struts>