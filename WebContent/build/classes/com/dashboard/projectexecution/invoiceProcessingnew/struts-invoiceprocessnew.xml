<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
    <package name="invoiceprocess" extends="struts-default">
			<action name="savedInvoice" class="com.dashboard.projectexecution.invoiceProcessing.ClsInvoiceProcessingAction" method="saveInvAction">
				<interceptor-ref name="params"/>
				<result name="success">invoiceprocessing.jsp</result>
				<result name="fail">invoiceprocessing.jsp</result>
				
			</action>

<action name="invoiceProcessingPrint" class="com.dashboard.projectexecution.invoiceProcessing.ClsInvoiceProcessingAction" method="printActionJasper">
				<interceptor-ref name="params"/>
				
			</action>

		</package>	
</struts>
