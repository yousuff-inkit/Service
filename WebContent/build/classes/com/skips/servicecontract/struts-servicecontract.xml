<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />   

	<package name="skip_ServiceContract" extends="struts-default" namespace="/">     
			<action name="saveServiceContractSkips" class="com.skips.servicecontract.ClsServiceContractAction" method="saveAction">       
				<interceptor-ref name="params"/>  
				<result name="success" >servicecontract.jsp</result>
				<result name="fail">servicecontract.jsp</result>   
			</action>
            <action name="printServiceContractSkip" class="com.skips.servicecontract.ClsServiceContractAction" method="printAction">    
					<interceptor-ref name="params"/>
					<result name="print" >${url}</result> 
			   </action>
	 </package>	 
	 <package name="ServCont-FileUpload" extends="json-default" namespace="/"> 
			 <action name="fileUploadServCont" class="com.skips.servicecontract.ClsServContAttach">   
				<param name="contentType">text/html</param>
	            <result type="json" name="success">  
	                <param name="contentType">text/html</param>  
	            </result>  
	            <result type="json" name="error">  
	                <param name="contentType">text/html</param>  
	            </result> 
			</action>  
	 </package>	 
</struts>     