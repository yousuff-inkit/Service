<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="skip_Quotation" extends="struts-default" namespace="/">  
			<action name="saveQuotationSkips" class="com.skips.quotation.ClsQuotationAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >quotation.jsp</result>
				<result name="fail">quotation.jsp</result>
			</action>  
			<action name="printQuotationSkip" class="com.skips.quotation.ClsQuotationAction" method="printAction">    
					<interceptor-ref name="params"/>
					<result name="print" >${url}</result> 
			   </action> 
		</package>	
</struts>