<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="changesdone" extends="struts-default">
			<action name="saveChangesDone" class="com.gwinternal.operations.changesdone.ClsChangesDoneAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">changesDone.jsp</result>
				<result name="fail">changesDone.jsp</result>
			</action>
			
		</package>	
</struts>