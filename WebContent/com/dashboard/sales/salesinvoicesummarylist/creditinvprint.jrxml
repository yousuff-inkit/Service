if(m.ref_type='DEL','Delivery Note','Sales Order')) type,  m.doc_no,m.voc_no,m.refno, DATE_FORMAT(m.date,'%d.%m.%Y') AS date,DATE_FORMAT(m.entrydate, '%d/%m/%Y %H:%i') entrydate,DATE_FORMAT(m.duedate,'%d.%m.%Y') AS duedate,h.description descs,h.account,
m.acno,m.curId,m.rate,m.amount,m.disper, m.discount,round(m.netAmount,2) netAmount,m.delterms,m.payterms,m.description,m.rrefno,b.tinno,ac.trnnumber,
b.branchname,coalesce(c.email,' ') email,c.company,coalesce(c.address,' ') caddress,coalesce(c.tel,' ') tel,
coalesce(c.fax,' ') fax,l.loc_name location,us.user_id,
round(sum(total),2)total, round(sum(d.discount),2) discount,round(sum(taxamount),2) taxamount,
round(sum(nettaxamount),2) nettotal, if(desc1='CTN',coalesce(dd.refno,''),'') ctn,
if(desc1='BAG',coalesce(dd.refno,''),'') bag,if(desc1!='CTN' && desc1!='BAG' ,coalesce(dd.refno,''),'') nos
from my_invm m left join my_head h on h.doc_no=m.acno
left join my_salm mm on mm.doc_no=m.sal_id
left join my_acbook ac on ac.cldocno=m.cldocno and ac.dtype='CRM'
left join my_area ar on ar.doc_no=ac.area_id
left join my_acountry cu on cu.doc_no=ar.country_id
left join my_shipdetails sh on sh.doc_no=m.shipdetid and sh.type>0
left join my_acbook ac1 on(ac1.doc_no=sh.cldocno and ac1.dtype='CRM')
left join my_invd d on m.doc_no=d.rdocno  left join my_brch b on m.brhid=b.doc_no
left join my_locm l on m.locid=l.doc_no
left join my_user us on m.userid=us.doc_no
left join my_comp c on b.cmpid=c.doc_no
left join my_deldetaild dd on dd.rdocno=m.doc_no
where   m.doc_no in ($P{printdocnos0},$P{printdocnos1},$P{printdocnos2},$P{printdocnos3},$P{printdocnos4},$P{printdocnos5},$P{printdocnos6},$P{printdocnos7},$P{printdocnos8},$P{printdocnos9}) group by m.doc_no;]]>
