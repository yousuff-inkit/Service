		<queryString>
			<![CDATA[select @row := @row + 1 as row,round(((a.qty*a.amount)+((a.qty*a.amount)*(a.taxper/100))),2) nettaxamount,
round(((a.qty*a.amount)*(a.taxper/100)),2) taxamount,  mrp, rate, taxper, qty, foc, batch, exp, productname,
productid from ( select  round(mm.mrp,2) mrp,  d.amount,round(d.amount,2) rate,round(d.taxper,2)  taxper,
round(if(d.qty<dd.qty,d.qty,dd.qty),2) qty,round(dd.foc,2) foc,dd.batch,dd.exp,  mm.productname ,   mm.part_no productid
from my_invd d  left join (select sum(qty) qty,sum(foc) foc,rdocno,orddocno,batch_no batch,  DATE_FORMAT(coalesce(exp_date,''), '%m/%y') exp from my_sorderddet
    where rdocno=$P{ordno} group by psrno,batch_no,orddocno)  dd on  dd.orddocno=d.ref_row
inner join my_main mm on d.psrno=mm.psrno where d.rdocno=$P{docno}  order by mm.part_no) a,(SELECT @row := 0) r;]]>
		</queryString>
