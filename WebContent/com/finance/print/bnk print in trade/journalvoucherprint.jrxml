			<![CDATA[SELECT t.account,t.description accountname,j.description accdesc,c.code currency,if(j.dramount>0,round((j.dramount*1),2),'  ') debit,
		if(j.dramount<0,round((j.dramount*-1),2),'  ') credit FROM my_jvma m left join my_jvtran j on m.tr_no=j.tr_no 
		left join my_head t on j.acno=t.doc_no left join my_curr c on c.doc_no=j.curId WHERE m.dtype='JVT' and m.brhid=$P{brch} 
