		<queryString>
			<![CDATA[select a.*,round($P{opnbal}-coalesce(@i:=@i+a.total,0),2) netamount from(SELECT coalesce(ma.refname,'') vendor,d.invno,date_format(d.invdate,'%d-%m-%Y')invdate,t.account accounts,
                     t.description accountname1,round(d.total,2)total,d.description desc1 FROM my_mcpbm m
                     left join my_mcpbd d on m.tr_no=d.tr_no left join my_costunit f on d.costtype=f.costtype
                     left join my_acbook ma on d.vendid=ma.doc_no and ma.dtype='VND' left join my_head t on d.acno=t.doc_no
                     left join my_curr c on c.doc_no=d.curId WHERE m.dtype='MCP' and m.brhId=$P{brch} and m.doc_no=$P{doc} and d.SR_NO>=0 order by d.SR_NO)a,(select @i:=0) as i;]]>
