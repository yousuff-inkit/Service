		<parameter name="docno" class="java.lang.String"/>
		<queryString>
			<![CDATA[select @i:=@i+1 row,a.* from(select  productname as instdesc,bd.brandname make,cl.category as model, serials as serial, round(d.rate,2)rate,qty, d.amount as total from cl_qotd d left join cl_qotm qm on d.rdocno=qm.doc_no   left join my_main m on(d.prodpsrno=m.doc_no) left join my_prodattrib at on(at.mpsrno=m.doc_no) left join  my_brand bd on m.brandid=bd.doc_no left join my_acbook a on qm.cldocno=a.cldocno and a.dtype='CRM'  left join my_clcatm cl on cl.doc_no=a.catid where rdocno=$P{docno} group by  d.prodpsrno,d.amount having sum(qty)>0  order by d.rowno)a,(select @i:=0)c]]>
		</queryString>
