		<![CDATA[SELECT x.*,ROUND(COALESCE(vataedrt/rate,0)+amountrt,2) totamountaedrt FROM(SELECT a.*,b.bankname, b.bankaddress, b.beneficiary, b.account, b.ibanno, b.swiftcode, b.benaddress, b.bankcur FROM(SELECT ROUND(COALESCE((@amountaedextot := @amountaedextot + amountaedex),0),2) AS amountaedexrt, ROUND(COALESCE((@vataedtot := @vataedtot + vataed),0),2) AS vataedrt, ROUND(COALESCE((@amountaedinctot := @amountaedinctot + amountaedinc),0),2) AS amountaedincrt,ROUND(COALESCE((@amounttot := @amounttot + amount),0),2) AS amountrt,c.* FROM(select @i:=@i+1 sno,t.*,ROUND((amountaedex*vat)/100,2) vataed,ROUND(((amountaedex*vat)/100)+amountaedex,2) amountaedinc FROM(SELECT 1 conid, c.code,ROUND(d.nettotal,2) amount,ROUND(d.nettotal*m.rate,2) amountaedex,ROUND(d.taxper,2) vat,DATE_FORMAT(m.date,'%d.%m.%Y') date,ac.refname,CONCAT(LPAD(m.voc_no,3,0),' / ',YEAR(m.date)) voc_no1,m.voc_no,ac.address,ac.trnnumber,m.payterm,br.imgpath, br.imgfooter,m.doc_no,m.rate,m.curid, br.tinno FROM my_srvsaleprom m LEFT JOIN my_brch br ON br.doc_no=m.brhid LEFT JOIN my_acbook ac on ac.acno=m.acno AND ac.dtype='CRM' left join my_srvsaleprod d ON d.rdocno=m.doc_no left join my_curr c on c.doc_no=m.curid WHERE m.doc_no=$P{maindocno})t,(select @i:=0)k)c,(select @amounttot:=0)d,(select @amountaedextot:=0)e,(select @vataedtot:=0)f,(select @amountaedinctot:=0)g)a LEFT JOIN (select 1 conid,name bankname,address bankaddress,beneficiary,account,ibanno,swiftcode,benaddress,bankcur FROM cm_bankdetails WHERE status=3 AND doc_no=$P{bankdocno})b ON b.conid=a.conid ORDER BY sno DESC LIMIT 1)x]]>
